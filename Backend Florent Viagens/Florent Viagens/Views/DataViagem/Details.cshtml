@model Florent_Viagens.Models.DataViagem

@{
    ViewData["Title"] = "Details";
}



<div>
    <h4 class="text-success text-center mt-5">Data da Viagem</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2 text-success">
            @Html.DisplayNameFor(model => model.Data)
        </dt>
        <dd class = "col-sm-10 text-success">
            @Html.DisplayFor(model => model.Data)
        </dd>
        <dt class = "col-sm-2 text-success">
            @Html.DisplayNameFor(model => model.Usuario)
        </dt>
        <dd class = "col-sm-10 text-success">
            @Html.DisplayFor(model => model.Usuario.Cpf)
        </dd>
        <dt class = "col-sm-2 text-success">
            @Html.DisplayNameFor(model => model.Destino)
        </dt>
        <dd class = "col-sm-10 text-success">
            @Html.DisplayFor(model => model.Destino.Descricao)
        </dd>
    </dl>
</div>
<div>
    <a class="text-white btn btn-success"  asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a class="text-white btn btn-success"  asp-action="Index">Voltar</a>
</div>
